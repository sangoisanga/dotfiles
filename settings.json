{
  "vim.vimrc.enable": true,
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "git.autofetch": true,
  "vim.history": 10000,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
  },
  "vim.highlightedyank.color": "rgba(250, 240, 170, 0.5)",
  "vim.highlightedyank.duration": 200,
  "vim.replaceWithRegister": true,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "ctrl+w",
        "m"
      ],
      "commands": [
        "workbench.action.toggleEditorWidths"
      ]
    },
    {
      "before": [
        "ctrl+w",
        "="
      ],
      "commands": [
        "workbench.action.editorLayoutSingle"
      ]
    },
    {
      "before": [
        "ctrl+w",
        "["
      ],
      "commands": [
        "workbench.action.moveEditorToLeftGroup"
      ]
    },
    {
      "before": [
        "ctrl+w",
        "]"
      ],
      "commands": [
        "workbench.action.moveEditorToRightGroup"
      ]
    },
    {
      "before": [
        "ctrl+w",
        "w"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ]
    },
    {
      "before": [
        "g",
        "s"
      ],
      "commands": [
        "editor.showSupertypes"
      ]
    },
    {
      "before": [
        "g",
        "h"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "g",
        "a"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": [
        "g",
        "y"
      ],
      "commands": [
        "workbench.action.showAllSymbols"
      ]
    },
    {
      "before": [
        "g",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "n"
      ],
      "commands": [
        "workbench.action.terminal.newInActiveWorkspace"
      ]
    },
    {
      "before": [
        "<leader>",
        "o",
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
      ],
      "commands": [
        "workbench.files.action.showActiveFileInExplorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "z",
      ],
      "commands": [
        "workbench.action.toggleZenMode"
      ]
    },
    {
      "before": [
        "<leader>",
        "x",
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "]",
        "c",
      ],
      "commands": [
        "workbench.action.editor.nextChange"
      ]
    },
    {
      "before": [
        "[",
        "c",
      ],
      "commands": [
        "workbench.action.editor.previousChange"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
  ],
  "vim.commandLineModeKeyBindings": [],
  "explorer.fileNesting.enabled": false,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.*.ts, ${capture}.d.ts, ${capture}.ts.meta",
    "*.js": "${capture}.js.*, ${capture}.*.js, ${capture}.d.ts",
    "*.yml": "${capture}.*.yml, ${capture}.*.yaml",
    "*.yaml": "${capture}.*.yml, ${capture}.*.yaml"
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "terminal.integrated.cursorBlinking": true,
  "zenMode.hideTabs": false,
  "zenMode.hideLineNumbers": false,
  "workbench.colorTheme": "WebStorm Darcula Darker Theme",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotionKeys": "hklyuiopnm,qwertzxcvbasdgjf",
  "files.autoSave": "onFocusChange",
  "editor.fontWeight": "normal",
  "editor.fontFamily": "Fira Code",
  "explorer.confirmDelete": false,
  "editor.minimap.enabled": false,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Cygwin": {
      "path": "C:\\cygwin64\\bin\\bash.exe",
      "args": [
        "--login"
      ]
    }
  },
  "extensions.ignoreRecommendations": true,
  "vim.foldfix": true,
  "eslint.enable": true,
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.codeActionsOnSave.mode": "problems",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true,
  },
  "editor.formatOnSaveMode": "modifications",
  "sort-imports.on-save": false
}